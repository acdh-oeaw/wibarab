{
  "name": "wibarab",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "next build",
    "cm": "git-cz",
    "cms:server": "netlify-cms-proxy-server",
    "dev": "next dev",
    "format": "prettier . --list-different --ignore-path .gitignore --loglevel warn",
    "formatfix": "yarn run format --write",
    "generate:favicons": "cross-env NODE_OPTIONS='--experimental-vm-modules' npx tsm scripts/generate-favicons.ts",
    "generate:feed": "cross-env NODE_OPTIONS='--experimental-vm-modules' npx tsm scripts/generate-feed.ts",
    "generate:route-manifest": "cross-env NODE_OPTIONS='--experimental-vm-modules' npx tsm scripts/generate-route-manifest.ts",
    "lint": "yarn run lint:code && yarn run lint:style",
    "lint:fix": "yarn run lint:code:fix && yarn run lint:style:fix",
    "lint:code": "next lint --dir . --ignore-path .gitignore",
    "lint:code:fix": "yarn run lint:code --fix",
    "lint:md": "remark . --quiet --frail",
    "lint:md:fix": "yarn run lint:md --output",
    "lint:mdx": "remark . --use mdx --quiet --frail",
    "lint:mdx:fix": "yarn run lint:mdx --output",
    "lint:style": "stylelint '**/*.css' --cache --cache-location .next/cache/stylelint/ --ignore-path .gitignore",
    "lint:style:fix": "yarn run lint:style --fix",
    "postbuild": "yarn run generate:favicons && yarn run generate:feed",
    "prebuild": "yarn run generate:route-manifest",
    "prepare": "simple-git-hooks || true",
    "start": "next start",
    "test": "jest --passWithNoTests",
    "test:ci": "yarn run test --ci",
    "test:coverage": "yarn run test --coverage",
    "test:watch": "yarn run test --watch",
    "type-check": "tsc -p ./tsconfig.json --noEmit",
    "validate": "yarn run format && yarn run lint && yarn run type-check && yarn run test"
  },
  "packageManager": "yarn@1",
  "engines": {
    "node": ">=14.17",
    "yarn": "1.x"
  },
  "dependencies": {
    "@mdx-js/loader": "^2.0.0-rc.1",
    "@stefanprobst/next-page-metadata": "^2.0.4",
    "@stefanprobst/next-svg": "^3.0.1",
    "clsx": "^1.1.1",
    "next": "^12.0.3",
    "react": "^18.0.0-alpha-ee069065d-20211105",
    "react-dom": "^18.0.0-alpha-ee069065d-20211105",
    "remark-frontmatter": "^4.0.1",
    "sharp": "^0.29.2",
    "vfile": "^5.2.0",
    "vfile-matter": "^3.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^14.1.0",
    "@commitlint/cz-commitlint": "^14.1.0",
    "@jest/types": "^27.2.5",
    "@next/eslint-plugin-next": "^12.0.2",
    "@stefanprobst/commitlint-config": "^1.0.2",
    "@stefanprobst/eslint-config-next": "^4.0.1",
    "@stefanprobst/favicons": "^2.0.2",
    "@stefanprobst/prettier-config": "^2.0.0",
    "@stefanprobst/remark-extract-yaml-frontmatter": "^2.2.1",
    "@stefanprobst/stylelint-config": "^2.0.0",
    "@stefanprobst/tsconfig": "^1.1.1",
    "@tailwindcss/typography": "^0.5.0-alpha.2",
    "@testing-library/dom": "^8.10.1",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@types/mdx": "^2.0.1",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.33",
    "autoprefixer": "^10.4.0",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-next": "^12.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.3.1",
    "lint-staged": "^11.2.6",
    "netlify-cms-proxy-server": "^1.3.22",
    "postcss": "^8.3.11",
    "prettier": "^2.4.1",
    "remark-cli": "^10.0.0",
    "simple-git-hooks": "^2.7.0",
    "stylelint": "^14.0.1",
    "tailwindcss": "^3.0.0-alpha.1",
    "typescript": "^4.4.4",
    "xast-util-feed": "^1.2.0",
    "xast-util-to-xml": "^3.0.0"
  },
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      ">=0.75%",
      "not dead",
      "not op_mini all"
    ]
  },
  "commitlint": {
    "extends": "@stefanprobst/commitlint-config"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "eslintConfig": {
    "extends": "@stefanprobst/eslint-config-next",
    "parserOptions": {
      "extraFileExtensions": [
        ".mjs"
      ]
    },
    "rules": {
      "@next/next/no-img-element": "off"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.@(js|ts|tsx)",
      "!**/*.d.ts",
      "!**/node_modules/**"
    ],
    "coverageProvider": "v8",
    "injectGlobals": false,
    "moduleNameMapper": {
      "\\.module\\.css$": "identity-obj-proxy",
      "\\.css$": "<rootDir>/test/mocks/style.ts",
      "\\.(?:jpg|jpeg|png|gif|webp|avif|svg)$": "<rootDir>/test/mocks/next-image.ts",
      "^@/(.*)$": "<rootDir>/src/$1",
      "^~/(.*)$": "<rootDir>/$1"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup.ts"
    ],
    "testEnvironment": "jsdom",
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/.next/",
      "<rootDir>/out/"
    ],
    "transform": {
      "\\.(?:js|ts|tsx)$": [
        "babel-jest",
        {
          "presets": [
            "next/babel"
          ]
        }
      ]
    }
  },
  "lint-staged": {
    "*.@(js|ts|tsx)": [
      "eslint --cache --cache-location .next/cache/eslint/ --fix",
      "prettier --write"
    ],
    "*.css": [
      "stylelint --cache --cache-location .next/cache/stylelint/ --fix",
      "prettier --write"
    ],
    "*.md": [
      "remark --quiet --output --frail",
      "prettier --write"
    ],
    "*.mdx": [
      "remark --use mdx --quiet --output --frail",
      "prettier --write"
    ],
    "*.@(json|yml)": "prettier --write"
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {}
    }
  },
  "prettier": "@stefanprobst/prettier-config",
  "remarkConfig": {
    "plugins": [
      "frontmatter"
    ]
  },
  "simple-git-hooks": {
    "commit-msg": "yarn commitlint --edit",
    "pre-commit": "yarn lint-staged",
    "pre-push": "yarn run validate"
  },
  "stylelint": {
    "extends": "@stefanprobst/stylelint-config",
    "rules": {
      "selector-id-pattern": null
    }
  }
}
